<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="schema.TaxpayerEntity" table="Taxpayer" schema="" catalog="Podatkova">
        <id name="taxpayerId">
            <column name="Taxpayer_id" sql-type="int" not-null="true"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="idCode">
            <column name="Id_code" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>