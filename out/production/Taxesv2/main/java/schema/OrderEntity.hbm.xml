<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="schema.OrderEntity" table="Order" schema="" catalog="Podatkova">
        <id name="orderId">
            <column name="Order_id" sql-type="int" not-null="true"/>
        </id>
        <property name="registrationId">
            <column name="Registration_id" sql-type="int" not-null="true"/>
        </property>
        <property name="text">
            <column name="Text" sql-type="varchar" length="300" not-null="true"/>
        </property>
        <property name="writerName">
            <column name="WriterName" sql-type="varchar" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>